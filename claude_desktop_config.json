{
  "mcpServers": {
    "zabbix-mcp": {
      "command": "python",
      "args": ["src/zabbix_mcp/server.py"],
      "env": {
        "ZABBIX_API_URL": "http://your-zabbix-server/api_jsonrpc.php",
        "ZABBIX_USERNAME": "your-username",
        "ZABBIX_PASSWORD": "your-password"
      }
    }
  }
}
