# Zabbix MCP Configuration

# Required: Zabbix server hostname or IP address
ZABBIX_HOST=zabbix.instanceone.co

# Required: Zabbix API username
ZABBIX_USERNAME=Admin

# Required: Zabbix API password
ZABBIX_PASSWORD=zabbix

# Optional: Zabbix API port (default: 80)
ZABBIX_PORT=80

# Optional: Use HTTPS (default: false)
ZABBIX_HTTPS=false

# Optional: Verify SSL certificates (default: true)
ZABBIX_VERIFY_SSL=true
